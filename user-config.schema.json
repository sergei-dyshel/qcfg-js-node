{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "baseTempDir": {
      "description": "Path to prefix directory for temporary files",
      "type": "string"
    },
    "rsync": {
      "additionalProperties": false,
      "properties": {
        "defaultRemoteRsyncPath": {
          "description": "Default path to remote rsync binary file",
          "type": "string"
        }
      },
      "type": "object"
    },
    "syg": {
      "additionalProperties": false,
      "properties": {
        "execSource": {
          "description": "Shell file to source on remote when running syg.exec",
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "type": "object"
}
